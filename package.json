    "main": "index.js",
    "scripts": {
        "build:server": "webpack --config webpack.config.prod.js",
        "dev": "webpack --watch",
        "start": "node ./out/fastify/server.js",
        "type-check": "vue-tsc --noEmit",
        "build:ssr": "npm run build:ssr:client && npm run build:ssr:server",
