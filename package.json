        "@sinclair/typebox": "^0.24.1",
        "@vitejs/plugin-vue": "^3.1.0",
        "devalue": "^2.0.1",
        "fastify": "^4.1.0",
        "ky-universal": "^0.11.0",
        "prisma": "^4.0.0",
        "vue": "^3.2.37",
        "vue-router": "^4.0.16"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
